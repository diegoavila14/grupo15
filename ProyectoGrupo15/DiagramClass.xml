			<att name="name" type="String" visibility="足"/>
			<att name="publicURL" type="URL" visibility="+"/>
			<att name="authors" type="Author[1..5]" visibility="足"/>
		</attributes>
		<methods>
			<method name="addEntry" type="void" visibility="+">
