			<att name="name2" type="String" visibility="足"/>
			<att name="publicURL2" type="URL" visibility="+"/>
			<att name="authors2" type="Author[1..5]" visibility="足"/>
		</attributes>
		<methods>
			<method name="addEntry2" type="void" visibility="+">
				<param name="newEntry2" type="BlogEntry"/>
